<div class="row">
  <div class="col-lg-12">
    <table class="table table-stripped">
      <tr>
        <th><%= CnabFile.human_attribute_name("created_at") %></th>
        <th><%= CnabFile.human_attribute_name("status") %></th>
      </tr>

      <% @cnab_files.each do |cnab_file| %>
        <tr>
          <td><%= l cnab_file.created_at %></td>
          <td><%= cnab_file.status %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
